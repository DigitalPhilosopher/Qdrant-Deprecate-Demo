{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Start Demo",
            "type": "shell",
            "command": "docker-compose up --build -d",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        },
        {
            "label": "Stop Demo",
            "type": "shell",
            "command": "docker-compose down",
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}